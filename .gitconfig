[user]
	name = Username
	email = email@example.com
[credential]
	helper = cache --timeout 3600

[color]
  # Use colors in Git commands that are capable of colored output when outputting to the terminal
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[help]
  autocorrect = 1
[push]
  default = current
[branch "master"]
  remote = origin
  merge = refs/heads/master